{
  "keys": {
    "command": {
      "bind": {
        "ctrl-k": "jupyter-notebook:clear-cell-output",
        "ctrl-s": "jupyter-notebook:interrupt-kernel",
        "ctrl-r,a": "jupyter-notebook:run-all-cells-above",
        "ctrl-r,c": "jupyter-notebook:restart-kernel-and-clear-output"
      }
    }
  },
  "load_extensions": {
    "jupyter-js-widgets/extension": true
  },
  "MarkdownCell": {
    "cm_config": {
      "lineWrapping": true
    }
  },
  "CodeCell": {
    "cm_config": {
      "lineWrapping": true
    }
  }
}
