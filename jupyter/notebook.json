{
  "keys": {
    "command": {
      "bind": {
        "ctrl-k": "jupyter-notebook:clear-cell-output",
        "ctrl-shift-r": "jupyter-notebook:restart-kernel-and-clear-output",
        "ctrl-s": "jupyter-notebook:interrupt-kernel",
        "r,a": "jupyter-notebook:run-all-cells-above"
      },
      "unbind": [
        "r"
      ]
    }
  },
  "load_extensions": {
    "jupyter-js-widgets/extension": true
  }
}
